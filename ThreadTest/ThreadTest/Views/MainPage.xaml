             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="ThreadTest.Views.MainPage"
             Title="{Binding Title}">

    <StackLayout>
        <Label Text="Page A" x:Name="LabelA"/>
        <Button Text="Next page" Command="{Binding NextCommand}"/>
    </StackLayout>

</ContentPage>